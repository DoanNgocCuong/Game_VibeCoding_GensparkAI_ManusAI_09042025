        "lint": "eslint . --ext .ts,.tsx",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "clean": "rimraf dist"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "eslint": "^8.57.0",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "terser": "^5.29.2",
        "typescript": "^5.4.2",
        "vite": "^5.1.6"
    },
    "dependencies": {
        "phaser": "^3.80.1"
    }
}