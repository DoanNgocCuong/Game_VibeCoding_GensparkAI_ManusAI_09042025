{
    "name": "empire-english-demo",
    "description": "A Phaser 3 TypeScript game for learning English",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/empire-english-demo.git"
    },
    "author": "Your Name <your.email@example.com>",
    "license": "MIT",
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
        "preview": "vite preview",
